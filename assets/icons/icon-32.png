<svg width="128" height="128" viewBox="0 0 128 128" fill="none" xmlns="http://www.w3.org/2000/svg">
  <!-- Background Circle -->
  <circle cx="64" cy="64" r="60" fill="url(#gradient)" stroke="url(#borderGradient)" stroke-width="2"/>
  
  <!-- Clock Face -->
  <circle cx="64" cy="64" r="45" fill="rgba(15, 23, 42, 0.8)" stroke="rgba(148, 163, 184, 0.3)" stroke-width="1"/>
  
  <!-- Clock Markings -->
  <g stroke="rgba(148, 163, 184, 0.6)" stroke-width="2">
    <!-- 12 o'clock -->
    <line x1="64" y1="24" x2="64" y2="30" stroke-width="3"/>
    <!-- 3 o'clock -->
    <line x1="104" y1="64" x2="98" y2="64" stroke-width="3"/>
    <!-- 6 o'clock -->
    <line x1="64" y1="104" x2="64" y2="98" stroke-width="3"/>
    <!-- 9 o'clock -->
    <line x1="24" y1="64" x2="30" y2="64" stroke-width="3"/>
    
    <!-- Other markings -->
    <line x1="86.4" y1="31.6" x2="84.1" y2="35.1"/>
    <line x1="96.4" y1="41.6" x2="92.9" y2="43.9"/>
    <line x1="96.4" y1="86.4" x2="92.9" y2="84.1"/>
    <line x1="86.4" y1="96.4" x2="84.1" y2="92.9"/>
    <line x1="41.6" y1="96.4" x2="43.9" y2="92.9"/>
    <line x1="31.6" y1="86.4" x2="35.1" y2="84.1"/>
    <line x1="31.6" y1="41.6" x2="35.1" y2="43.9"/>
    <line x1="41.6" y1="31.6" x2="43.9" y2="35.1"/>
  </g>
  
  <!-- Clock Hands -->
  <g stroke="rgba(148, 163, 184, 0.9)" stroke-linecap="round">
    <!-- Hour hand pointing to around 11 -->
    <line x1="64" y1="64" x2="58" y2="40" stroke-width="4"/>
    <!-- Minute hand pointing to around 5 -->
    <line x1="64" y1="64" x2="85" y2="85" stroke-width="3"/>
  </g>
  
  <!-- Center dot -->
  <circle cx="64" cy="64" r="3" fill="rgba(148, 163, 184, 0.9)"/>
  
  <!-- Year text -->
  <text x="64" y="115" text-anchor="middle" fill="rgba(148, 163, 184, 0.8)" font-family="Inter, sans-serif" font-size="12" font-weight="600">2025</text>
  
  <!-- Gradient Definitions -->
  <defs>
    <linearGradient id="gradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:rgba(148, 163, 184, 0.15);stop-opacity:1" />
      <stop offset="100%" style="stop-color:rgba(148, 163, 184, 0.05);stop-opacity:1" />
    </linearGradient>
    <linearGradient id="borderGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:rgba(148, 163, 184, 0.3);stop-opacity:1" />
      <stop offset="100%" style="stop-color:rgba(148, 163, 184, 0.1);stop-opacity:1" />
    </linearGradient>
  </defs>
</svg>
